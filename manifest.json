{
  "manifest_version": 3,
  "name": "TBlocker",
  "version": "1.0",
  "description": "Blocks opening new tabs with specific URLs",
  "permissions": ["storage", "tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html"
  }
}
